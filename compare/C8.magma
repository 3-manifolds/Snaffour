R<a,b,c,d,e,f,g,h> := PolynomialRing(GF(2^31-1), 8, "grevlex");

C8 := [a*b*c*d*e*f*g*h - 1,
 a*b*c*d*e*f*g + a*b*c*d*e*f*h + a*b*c*d*e*g*h + a*b*c*d*f*g*h + a*b*c*e*f*g*h + a*b*d*e*f*g*h + a*c*d*e*f*g*h + b*c*d*e*f*g*h,
 a*b*c*d*e*f + b*c*d*e*f*g + a*b*c*d*e*h + a*b*c*d*g*h + a*b*c*f*g*h + a*b*e*f*g*h + a*d*e*f*g*h + c*d*e*f*g*h,
 a*b*c*d*e + b*c*d*e*f + c*d*e*f*g + a*b*c*d*h + a*b*c*g*h + a*b*f*g*h + a*e*f*g*h + d*e*f*g*h,
 a*b*c*d + b*c*d*e + d*e*f*g + a*b*c*h + a*b*g*h + a*f*g*h,
 a*b*c + b*c*d + c*d*e + d*e*f + e*f*g + a*b*h + a*g*h,
 a*b + b*c + c*d + d*e + e*f + f*g + a*h + g*h,
 a + c + d + e + f + g + h];

SetVerbose("Faugere", 3);

time G := GroebnerBasis(C8);

# G;

quit;
